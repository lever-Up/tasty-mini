page{
  color: #2A2A2A;
}
.me-info-wrapper{
  padding-left: 30rpx;
  padding-right: 30rpx;
  height: 320rpx;
  background: linear-gradient(to right bottom, #FFF1F1, #58B7FF);
}
.me-info{
  margin-top: 40rpx;
  background: transparent;
  border: none;
}
.me-info::after{
  border: none
}
.me-name{
  font-weight: bold;
  font-size: 35rpx;
  letter-spacing: 10rpx;
}
.icon-vip{
  width: 40rpx;
  height: 40rpx;
  background-size: contain;
  background-repeat: no-repeat;
  background-image: url("data:image/svg+xml;base64,");
}
.me-icon{
  overflow: hidden;
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
  margin-right: 30rpx;
}
.wx-icon{
  width: 90rpx;
  height: 90rpx;
  background: url("data:image/svg+xml;base64,") no-repeat;
  background-size:90rpx 100%;
  margin-right: 30rpx;
}
.me-values{
  position: absolute;
  height: 180rpx;
  width: 680rpx;
  bottom: -80rpx;
  left: 50%;
  transform: translateX(-50%);
  background-color: #ffffff;
  border-radius: 20rpx;
  padding: 0 10rpx;
  justify-content: space-around;
}
.me-number{
  font-family: Gobold;
  font-size: 30rpx;
  font-weight: bold;
}
.me-values-item{
  position: relative;
  width: 200rpx;
  text-align: center;
}
.me-values-item:last-child::after{
  display: none;
}
.me-values-item::after{
  content: '';
  position: absolute;
  top: 15rpx;
  bottom: 15rpx;
  right: 0;
  width: 1rpx;
  background-color: #E7E7E7;
}
.me-values-desc{
  margin-top: 15rpx;
  font-size: 24rpx;
  color: #676767;
}

.wrapper-card{
  margin: 20rpx 30rpx;
  padding: 30rpx;
  background-color: #ffffff;
  border-radius: 20rpx;
}
.wrapper-title{
  justify-content: space-between;
  align-items: flex-end;
}
.wrapper-item-title{
  font-size: 38rpx;
}

.me-orders{
  margin-top: 100rpx;
}
.all-orders, .icon-right{
  font-size: 26rpx;
  color: #B0B0B0;
}
.order-wrapper{
  justify-content: space-around;
  padding: 30rpx 0;
}
.order-status{
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
  font-size: 24rpx;
  color: #676767;
}
.order-icon{
  background-size: contain;
  background-repeat: no-repeat;
  width: 55rpx;
  height: 55rpx;
  margin-bottom: 20rpx;
}
.waiting-pay-icon{
  background-image: url("data:image/svg+xml;base64,");
}
.waiting-receive-icon{
  background-image: url("data:image/svg+xml;base64,");
}
.sending-icon{
  background-image: url("data:image/svg+xml;base64,");
}
.finish-icon{
  background-image: url("data:image/svg+xml;base64,");
}

.service-wrapper{
  flex-wrap: wrap;
  margin-top: 30rpx;
}
.series-item{
  display: flex;
  width: 33%;
  align-items: center;
  justify-content: center;
  flex-direction: column;
  font-size: 24rpx;
  color: #676767;
  padding-bottom: 20rpx;
}
.series-icon{
  background-size: contain;
  background-repeat: no-repeat;
  width: 55rpx;
  height: 55rpx;
  margin-bottom: 30rpx;
}

/* 优惠券 */
.discount-coupon{
  margin: 0 30rpx 20rpx;
  padding-bottom: 20rpx;
  background-color: #FFF;
}
.ticket-coupon{
  width: 100%;
  white-space: nowrap;
}
.ticket-block{
  display: inline-block;
}
