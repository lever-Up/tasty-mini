.vip-room{
  width: 200%;
  height: 490rpx;
  background-image: linear-gradient(-218deg, #FFC39F 0%, #FF874C 100%);
  overflow-x: hidden;
  border-bottom-right-radius: 50%;
  border-bottom-left-radius: 50%;
  transform: translateX(-25%);
}
.header-content{
  margin: 0 25%;
  height: 100%;
}
.header-content-item{
  padding: 20rpx 20rpx 0 20rpx;
}
.user-info{
  justify-content: space-between;
}
.user-icon{
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  margin-right: 10rpx;
}
.user-name{
  font-size: 32rpx;
  color: #ffffff;
  font-weight: bold;
  letter-spacing: 10rpx;
}
.vip-deadline{
  margin-top: 10rpx;
  font-size: 20rpx;
  color: #ffffff;
}
.icon-vip{
  display: inline-block;
  width: 40rpx;
  height: 40rpx;
  background-size: contain;
  background-repeat: no-repeat;
  background-image: url("data:image/svg+xml;base64,");
}

.vip-description{
  padding: 30rpx 0;
  font-size: 24rpx;
  color: #fff;
}
.icon-questionfill{
  font-size: 24rpx;
  margin-left: 10rpx;
}
.vip-privilege{
  z-index: 1;
  padding: 30rpx;
  background-color: #ffffff;
  border-radius: 10rpx;
  position: absolute;
  top: 280rpx;
  left: 20rpx;
  right: 20rpx;
  font-size: 30rpx;
  opacity: .9;
}
.privilege-title{
  font-size: 34rpx;
  font-weight: bold;
  text-align: center;
  text-shadow: 4rpx 4rpx #FFF1F1;
  margin-bottom: 10rpx;
}
.privilege-options-num{
  font-family: Gobold;
  color: #E41919;
  margin: 0 10rpx;
}
.privilege-values{
  font-size: 24rpx;
  text-decoration: underline;
  text-align: center;
  margin-bottom: 20rpx;
}
.icon-playfill{
  font-size: 24rpx;
}
.privilege-options{
  margin: 30rpx 0;
}
.privilege-item{
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 25%;
  font-size: 24rpx;
}
.privilege-item-icon{
  width: 60rpx;
  height: 60rpx;
  background-size: contain;
  background-repeat: no-repeat;
  margin-bottom: 15rpx;
}
.vip-card{
  background-image: url("data:image/svg+xml;base64,");
}
.active-icon{
  background-image: url("data:image/svg+xml;base64,");
}
.answer-icon{
  background-image: url("data:image/svg+xml;base64,");
}
.score-icon{
  background-image: url("data:image/svg+xml;base64,");
}
.privilege-being{
  background-color: #FF874C;
  color: #ffffff;
  text-align: center;
  border-radius: 50rpx;
  padding: 10rpx 20rpx;
}
.privilege-btn-name{
  font-size: 38rpx;
  margin-bottom: 5rpx;
}
.privilege-daily-price{
  font-size: 24rpx;
  text-decoration: underline;
}
.active-banner{
  margin-top: 90rpx !important;
}
.privilege-card{
  background-color: #ffffff;
  margin: 10rpx 30rpx;
  border-radius: 10rpx;
  padding: 20rpx;
  box-sizing: content-box;
  font-size: 30rpx;
}
.banner-swipe{
  width: 100%;
  height: 200rpx;
}
.privilege-banner-image{
  width: 100%;
  height: 100%;
  background-color: #E7E7E7;
  border-radius: 8rpx;
}
.banner-index{
  position: absolute;
  bottom: 30rpx;
  right: 30rpx;
  padding: 5rpx 10rpx;
  font-size: 20rpx;
  border-radius: 5rpx;
  background-color: rgba(0, 0, 0, .7);
  color: #fff;
}
.privilege-ana{
  color: #E41919;
  font-size: 22rpx;
  padding: 3rpx 30rpx 3rpx 10rpx;
  background: linear-gradient(to right, #FFF1F1, #ffffff);
}
.card-title{
  align-items: flex-end;
  margin-bottom: 10rpx;
}
.privilege-ana{
  font-size: 22rpx;
  padding: 0 5rpx;
  margin-left: 10rpx;
}
.privilege-card-image{
  width: 100%;
  height: 200rpx;
  background-color: #E7E7E7;
  border-radius: 8rpx;
}

.fixed-bottom{
  position: fixed;
  bottom: 50rpx;
  left: 30rpx;
  right: 30rpx;
}

.vip-description-dialog{
  position: fixed;
  top: 0;
  right: 0;
  left: 0;
  bottom: 0;
  background: rgba(0, 0, 0, .8);
  z-index: 2;
}
.dialog-content{
  position: fixed;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  background-color: #ffffff;
  border-radius: 30rpx;
  width: 550rpx;
  height: 720rpx;
  overflow-y: scroll;
  border: 1rpx solid rgba(255, 200, 200, .5);
  box-shadow: 0 0 20rpx rgba(255, 241, 241, .9);
  padding: 30rpx;
  font-size: 26rpx;
  color: #2A2A2A;
  letter-spacing: 5rpx;
  line-height: 50rpx;
  transition: transform 1s;
}
