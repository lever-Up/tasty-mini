.credit-room{
  background-image: linear-gradient(-218deg, #FFC39F 0%, #FF874C 100%);
}
.user-info{
  justify-content: space-between;
  padding: 50rpx;
}
.user-icon{
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  margin-right: 10rpx;
}
.user-name{
  font-size: 32rpx;
  color: #ffffff;
  font-weight: bold;
  letter-spacing: 10rpx;
}
.vip-deadline{
  margin-top: 10rpx;
  font-size: 20rpx;
  color: #ffffff;
}
.icon-vip{
  display: inline-block;
  width: 40rpx;
  height: 40rpx;
  background-size: contain;
  background-repeat: no-repeat;
  background-image: url("data:image/svg+xml;base64,");
}
.score-description{
  color: #ffffff;
  text-align: center;
}
.score-num{
  font-family: Gobold;
  font-size: 35rpx;
  margin-top: 10rpx;
}

.credit-products-wrapper{
  background-color: #ffffff;
  padding-left: 30rpx;
  margin-top: 20rpx;
}
.credit-products-list{
  padding-top: 30rpx;
}
.credit-products-item{
  width: 338rpx;
  float: left;
  margin-right: 20rpx;
  margin-bottom: 20rpx;
  border-radius: 10rpx;
  box-shadow: 0 0 20rpx rgba(0, 0, 0, .5);
}
.credit-products-item-image{
  width: 100%;
  height: 350rpx;
  border-top-left-radius: 10rpx;
  border-top-right-radius: 10rpx;
}
.credit-products-item-title{
  padding: 10rpx 20rpx;
  font-size: 30rpx;
  text-align: center;
}
.credit-products-item-flower{
  font-size: 26rpx;
  padding: 0 20rpx 30rpx;
  justify-content: space-between;
}
.score-num{
  font-family: Gobold;
}
.has-song{
  color: #676767;
}
